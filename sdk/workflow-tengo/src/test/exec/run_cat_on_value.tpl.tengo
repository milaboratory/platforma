/** Runs a cat on the file that was created from a content. */

self := import(":tpl")
smart := import(":smart")
exec := import(":exec")
file := import(":file")

self.defineOutputs(["main"])

self.body(func(inputs) {
	run := exec.builder().
		cmd("/usr/bin/env").
		arg("bash").
		arg("-c").
		arg("cat input.txt").
		writeFile("input.txt", 42).
		saveStdoutContent().
		run()

	result := {
		main: run.getStdoutFileContent()
	}

	return result
})
