ll := import("@milaboratory/tengo-sdk:ll")
wf := import(":workflow")
render := import(":render")

concat_ab := ll.getTemplateId(":test.concat_ab")

wf.body(func(args) {

	run := render.create(concat_ab, {
		a: args.a,
		b: args.b
	})

	return ll.toStrict({
		outputs: {
			outputResult: run.output("concat")
		},

		exports: {}
	})
})