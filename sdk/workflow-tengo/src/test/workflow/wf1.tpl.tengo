// simple wf with pre-run

wf := import(":workflow")
ll := import(":ll")


wf.setPreRun(ll.importTemplate(":test.workflow.wf1-prerun"))


wf.body(func(args) {
	testValue := args.testValue

	result := args.testValue + " Show Run"

	return {
		outputs: {
			outputResult: result
		},

		exports: {}
	}
})
