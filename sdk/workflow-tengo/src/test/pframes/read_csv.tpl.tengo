// read_pf_from_csv.tpl

self := import(":tpl")
exec := import(":exec")
xsv := import(":pframes.xsv")

self.defineOutputs("pf")

self.body(func(inputs){

	// csv as text
	csv := inputs.csv

	e := exec.builder().
		cmd("env").
		writeFile("data.csv", csv).
		saveFile("data.csv").
		run()

	csvFile := e.getFile("data.csv")

	// pfconv params
	params := inputs.params

	return {
		pf: xsv.importFile(csvFile, "csv", params)
	}
})
