ll := import(":ll")
maps := import(":maps")

assert := func(got, want) {
	ll.assert(got == want, "got: %#v, want: %#v", got, want)
}

TestDeepMerge := func() {
	assert(
		maps.deepMerge(
			{a: {a: 1, b: 2}, c: 1, d: {a: {b: {c: 12}, d: 1}}},
			{a: {b: 3}, d: {a: {b: {c: undefined, d: 23}, d: 1}}}
		),
		{a: {a: 1, b: 3}, c: 1, d: {a: {b: {d: 23}, d: 1}}}
	)
}
