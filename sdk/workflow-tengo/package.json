{
  "name": "@platforma-sdk/workflow-tengo",
  "version": "1.2.5",
  "description": "Tengo SDK library",
  "files": [
    "./dist/**/*",
    "./index.d.ts",
    "./index.js"
  ],
  "scripts": {
    "type-check": "pl-tengo check",
    "build": "rm -rf dist && pl-tengo check && pl-tengo build",
    "test": "pl-tengo test",
    "format": "/usr/bin/env emacs --quick --script ./format.el",
    "format-stdin": "/usr/bin/env emacs --quick --script ./format-stdin.el",
    "vscode-tags": "./create_tags.sh .tags false",
    "emacs-tags": "./create_tags.sh ../TAGS false -e"
  },
  "dependencies": {
    "@milaboratories/software-pframes-conv": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/tengo-template-builder": "workspace:^"
  }
}
