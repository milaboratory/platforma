  },
  "files": [
    "dist",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "check": "pl-tengo check",
    "build": "rm -rf dist && pl-tengo check && pl-tengo build",
    "test": "rm -rf dist && pl-tengo check && pl-tengo build && pl-tengo test && vitest",
    "format": "/usr/bin/env emacs --quick --script ./format.el",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "vscode-tags": "./create_tags.sh .tags false",
    "emacs-tags": "./create_tags.sh ../TAGS false -e"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.3.3",
    "@types/node": "^20.16.1",
    "@milaboratory/tengo-template-builder": "^1.12.0",
    "@milaboratory/pl-middle-layer": "^1.9.29",
    "@milaboratory/sdk-test": "^1.4.15",
    "vitest": "^2.0.5"
  }
}
