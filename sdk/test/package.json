{
  "name": "@platforma-sdk/test",
  "version": "1.45.24",
  "description": "Typescript Block Test Helpers",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "ts-builder types --target node",
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "test": "vitest run",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratories/pl-middle-layer": "workspace:*",
    "@platforma-sdk/model": "workspace:*",
    "@milaboratories/pl-client": "workspace:*",
    "@milaboratories/pl-tree": "workspace:*",
    "@milaboratories/computable": "workspace:*",
    "@milaboratories/ts-helpers": "workspace:*",
    "vitest": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}