{
  "name": "@milaboratory/sdk-test",
  "version": "1.4.4",
  "description": "Typescript Block Test Helpers",
  "type": "module",
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratory/pl-middle-layer": "^1.9.25",
    "vitest": "^2.0.5",
    "@milaboratory/pl-tree": "^1.3.3",
    "@milaboratory/computable": "^2.1.1",
    "@milaboratory/ts-helpers": "^1.0.19"
  },
  "devDependencies": {
    "@types/node": "~20.14.15",
    "prettier": "^3.2.5",
    "tsup": "~8.1.2",
    "typescript": "^5.5.4"
  },
  "tsup": {
    "entry": {
      "lib": "src/index.ts"
    },
    "splitting": false,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
