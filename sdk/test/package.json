{
  "name": "@milaboratory/sdk-test",
  "version": "1.2.3",
  "description": "Typescript Block Test Helpers",
  "type": "module",
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratory/pl-middle-layer": "^1.6.3",
    "vitest": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "prettier": "^3.2.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@milaboratory/pl-tree": "*",
    "@milaboratory/computable": "*",
    "@milaboratory/ts-helpers": "*"
  },
  "tsup": {
    "entry": {
      "lib": "src/index.ts"
    },
    "splitting": false,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
