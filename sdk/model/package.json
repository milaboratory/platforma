{
  "name": "@platforma-sdk/model",
  "version": "1.2.22",
  "description": "Platforma.bio SDK / Block Model",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "node ./scripts/save-package-version.cjs && tsup",
    "type-check": "node ./scripts/save-package-version.cjs && tsc --noEmit --composite false",
    "test": "node ./scripts/save-package-version.cjs && jest"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md"
  ],
  "dependencies": {
    "@milaboratories/pl-model-common": "workspace:^",
    "utility-types": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
