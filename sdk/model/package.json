{
  "name": "@platforma-sdk/model",
  "version": "1.44.1",
  "description": "Platforma.bio SDK / Block Model",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "ts-builder types --target node",
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "lint": "eslint .",
    "test": "vitest run",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md"
  ],
  "dependencies": {
    "@milaboratories/pl-model-common": "workspace:*",
    "@milaboratories/pl-error-like": "workspace:*",
    "utility-types": "catalog:",
    "canonicalize": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@platforma-sdk/eslint-config": "workspace:*",
    "@milaboratories/helpers": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "fast-json-patch": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}