{
  "name": "@milaboratories/ui-vue",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/lib.umd.cjs",
  "module": "dist/lib.js",
  "types": "dist/src/lib.d.ts",
  "styles": "dist/style.css",
  "exports": {
    ".": {
      "style": "./styles/main.css",
      "types": "./dist/src/lib.d.ts",
      "default": "./dist/lib.js"
    },
    "./styles": {
      "default": "./dist/style.css"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly -p tsconfig.lib.json",
    "watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit --project ./tsconfig.json",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@milaboratories/helpers": "workspace:^",
    "@platforma-sdk/model": "workspace:^",
    "vue": "^3.4.31"
  },
  "dependencies": {
    "@milaboratories/uikit": "workspace:^"
  },
  "devDependencies": {
    "@ag-grid-community/client-side-row-model": "^32.1.0",
    "@ag-grid-community/core": "^32.1.0",
    "@ag-grid-community/infinite-row-model": "^32.1.0",
    "@ag-grid-community/styles": "^32.1.0",
    "@ag-grid-community/vue3": "^32.1.0",
    "@ag-grid-enterprise/clipboard": "^32.1.0",
    "@ag-grid-enterprise/range-selection": "^32.1.0",
    "@faker-js/faker": "8.4.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "canonicalize": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.4",
    "lodash": "4.17.21",
    "sass": "1.77.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.6",
    "zod": "catalog:"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
