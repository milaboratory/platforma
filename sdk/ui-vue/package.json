{
  "name": "@platforma-sdk/ui-vue",
  "version": "1.42.26",
  "type": "module",
  "main": "dist/index.js",
  "styles": "dist/index.js",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./styles": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "dev": "ts-builder serve --target browser-lib",
    "build": "ts-builder build --target browser-lib",
    "watch": "ts-builder build --target browser-lib --watch",
    "type-check": "ts-builder types --target browser-lib",
    "preview": "vite preview",
    "lint": "eslint .",
    "check": "yarpm type-check && yarpm lint && yarpm test",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "dependencies": {
    "@types/node": "catalog:",
    "@types/semver": "catalog:",
    "@types/d3-format": "catalog:",
    "@milaboratories/biowasm-tools": "workspace:*",
    "@milaboratories/miplots4": "catalog:",
    "@milaboratories/uikit": "workspace:*",
    "@platforma-sdk/model": "workspace:*",
    "ag-grid-enterprise": "catalog:",
    "ag-grid-vue3": "catalog:",
    "canonicalize": "catalog:",
    "lru-cache": "catalog:",
    "vue": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/integrations": "catalog:",
    "d3-format": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/ts-configs": "workspace:*",
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/eslint-config": "workspace:*",
    "@milaboratories/helpers": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "happy-dom": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "rollup-plugin-sourcemaps2": "catalog:",
    "sass": "catalog:",
    "semver": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "@vue/test-utils": "catalog:",
    "yarpm": "catalog:",
    "fast-json-patch": "catalog:",
    "@faker-js/faker": "catalog:"
  }
}