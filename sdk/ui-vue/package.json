{
  "name": "@milaboratory/sdk-vue",
  "version": "1.0.31",
  "description": "vue sdk",
  "main": "lib/dist/lib.cjs",
  "module": "lib/dist/lib.js",
  "types": "lib/dist/src/lib.d.ts",
  "directories": {
    "lib": "lib"
  },
  "workspaces": [
    "lib",
    "playground"
  ],
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "build": "npm run build -w ./lib",
    "watch": "npm run watch -w ./lib",
    "type-check": "npm run type-check -w ./lib",
    "lint": "eslint . --ext js,ts,vue",
    "playground": "npm run dev -w playground",
    "prepare": "husky",
    "verify-package-lock": "tsx scripts/verify-package-lock.ts",
    "check": "npm run type-check && npm run lint && npm run verify-package-lock"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@milaboratory/helpers": "^1.5.1",
    "@milaboratory/platforma-uikit": "^1.0.13",
    "@milaboratory/sdk-ui": "^0.12.10",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.0",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.4",
    "sass": "1.77.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
