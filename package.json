{
  "name": "@milaboratories/platforma-monorepo",
  "private": true,
  "scripts": {
    "reset:node_modules": "find . -type d -name 'node_modules' | grep -v '\\.pnpm' | xargs rm -r",
    "reset:turbo-cache": "find . -type d -name '.turbo' | grep -v '\\.pnpm' | xargs rm -r",
    "reset": "pnpm run reset:node_modules && pnpm run reset:turbo-cache",
    "reset:etc-blocks": "rimraf etc/blocks/**/.turbo !**/.pnpm/** etc/blocks/**/ui/dist etc/blocks/**/model/dist --glob",
    "build": "turbo run build",
    "build:dev": "PL_PKG_DEV=local turbo run build",
    "build:dry-run": "turbo run build --dry-run=json",
    "watch-build": "turbo watch build",
    "test": "PL_PKG_DEV=local turbo run test --concurrency 1",
    "test:verbose": "PL_PKG_DEV=local turbo run test --concurrency 1",
    "test:pl-ci": "turbo run test --concurrency 1 --output-logs=errors-only",
    "test:dry-run": "PL_PKG_DEV=local turbo run test --dry-run=json",
    "test:no-cache": "PL_PKG_DEV=local turbo run test --concurrency 1 --force",
    "publish-to-local-registry": "turbo run publish-to-local-registry",
    "do-pack": "PL_PKG_DEV=local turbo run do-pack",
    "do-pack:remote": "turbo run do-pack",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "prepare": "husky",
    "fmt": "turbo run fmt",
    "check": "turbo run check",
    "formatter:check": "turbo run formatter:check",
    "linter:check": "turbo run linter:check",
    "types:check": "turbo run types:check",
    "ci:prepare": "pnpm --filter \"@milaboratories/ts-builder...\" run build && pnpm run formatter:check && pnpm run linter:check",
    "ci:build": "pnpm run ci:prepare && pnpm run build",
    "ci:build:dev": "pnpm run ci:prepare && pnpm run build:dev"
  },
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "@types/node": "catalog:",
    "@zed-industries/vscode-langservers-extracted": "catalog:",
    "husky": "catalog:",
    "rimraf": "catalog:",
    "rollup": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
  "pnpm": {
    "overrides": {
      "@microsoft/api-extractor>typescript": "catalog:"
    }
  },
  "//": {
    "@platforma-open/milaboratories.software-ptabler": "file:/Users/dbolotin/mi/core/ptabler/software/package.tgz",
    "overrides": {}
  }
}
