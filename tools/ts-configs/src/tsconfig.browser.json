{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "customConditions": ["sources"],
    "target": "esnext",
    "module": "preserve",
    "moduleResolution": "bundler",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true,
    "jsx": "preserve",
    "types": [
      "vite/client",
      "vitest/importMeta"
    ]
  }
}
