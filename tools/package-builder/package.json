{
  "name": "@platforma-sdk/package-builder",
  "version": "2.12.0",
  "description": "MiLaboratories Platforma Package builder",
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "oclif-index && vite build",
    "test": "jest && node ../../tests/package-builder/test.js"
  },
  "bin": {
    "pl-pkg": "./bin/run.mjs"
  },
  "files": [
    "bin/run.cmd",
    "bin/run.mjs",
    "dist"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/lib-storage": "catalog:",
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "tar": "catalog:",
    "winston": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@jest/globals": "catalog:",
    "@milaboratories/oclif-index": "workspace:*",
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/archiver": "^6.0.3",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "archiver": "^7.0.1",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "oclif": {
    "bin": "pl-pkg",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-pkg",
    "topicSeparator": " "
  }
}
