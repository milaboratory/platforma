{
  "version": "1.3.0",
  "name": "@milaboratories/build-configs",
  "type": "module",
  "deprecated": {
    "tsconfig_lib_bundled.json": "Use @milaboratories/ts-configs instead"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tsup": {
      "types": "./src/tsup.d.ts",
      "import": "./dist/tsup.js",
      "require": "./dist/tsup.cjs"
    },
    "./vite": {
      "types": "./src/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./tsconfig_lib_bundled.json": "./tsconfig_lib_bundled.json"
  },
  "files": [
    "./dist/**",
    "./tsconfig_lib_bundled.json"
  ],
  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit --composite false",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz",
    "lint": "eslint ."
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-json": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-typescript": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "vue": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-cleandir": "catalog:",
    "rollup-plugin-node-externals": "catalog:",
    "rollup-plugin-sourcemaps2": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vite-plugin-css-injected-by-js": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vite-plugin-externalize-deps": "^0.9.0"
  },
  "devDependencies": {
    "@milaboratories/eslint-config": "workspace:*",
    "eslint": "catalog:"
  }
}