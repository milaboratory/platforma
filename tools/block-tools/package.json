{
  "name": "@platforma-sdk/block-tools",
  "version": "2.1.5",
  "description": "Utility to manipulate Platforma Blocks and Block Registry",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "readme": "exec oclif readme",
    "test": "jest"
  },
  "bin": {
    "block-tools": "./bin/run.js"
  },
  "files": [
    "bin/*",
    "dist/**/*",
    "README.md"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratories/pl-model-middle-layer": "workspace:^",
    "@milaboratories/ts-helpers": "workspace:^",
    "@milaboratories/ts-helpers-oclif": "workspace:^",
    "@aws-sdk/client-s3": "catalog:",
    "@oclif/core": "catalog:",
    "mime-types": "catalog:",
    "semver": "catalog:",
    "tar": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/jest": "catalog:",
    "@types/mime-types": "catalog:",
    "@types/node": "catalog:",
    "@types/semver": "catalog:",
    "jest": "catalog:",
    "oclif": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "oclif": {
    "commands": {
      "strategy": "explicit",
      "target": "./dist/cli.js",
      "identifier": "COMMANDS"
    },
    "bin": "block-tools",
    "dirname": "block-tools",
    "topicSeparator": " "
  }
}
