{
  "name": "@platforma-sdk/tengo-builder",
  "version": "2.1.4",
  "description": "Pl Tengo Template Builder",
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit --composite false",
    "build": "oclif-index && vite build",
    "test": "vitest",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "bin": {
    "pl-tengo": "./bin/run.js"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:*",
    "@milaboratories/resolve-helper": "workspace:*",
    "@milaboratories/tengo-tester": "catalog:",
    "@milaboratories/pl-model-backend": "workspace:^",
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@milaboratories/eslint-config": "workspace:^",
    "@milaboratories/oclif-index": "workspace:*",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "@types/node": "catalog:"
  },
  "oclif": {
    "bin": "pl-tengo",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-tengo",
    "topicSeparator": " "
  }
}
