{
  "name": "@platforma-sdk/tengo-builder",
  "version": "2.4.4",
  "description": "Pl Tengo Template Builder",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "type-check": "ts-builder types --target node",
    "build": "oclif-index && ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "test": "vitest run --coverage",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "bin": {
    "pl-tengo": "./bin/run.js"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:*",
    "@milaboratories/resolve-helper": "workspace:*",
    "@milaboratories/tengo-tester": "catalog:",
    "@milaboratories/pl-model-backend": "workspace:*",
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/eslint-config": "workspace:*",
    "@milaboratories/oclif-index": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "@types/node": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  },
  "oclif": {
    "bin": "pl-tengo",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-tengo",
    "topicSeparator": " "
  }
}