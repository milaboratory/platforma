{
  "name": "@platforma-sdk/bootstrap",
  "version": "5.2.46",
  "description": "MiLaboratories Platforma Backend bootstrap code: configuration generators, service run wrappers and so on",
  "keywords": [],
  "license": "UNLICENSED",
  "bin": {
    "pl-dev": "./bin/run.js"
  },
  "files": [
    "./dist/**/*",
    "README.md",
    "bin/run.cmd",
    "bin/run.js",
    "./assets/**",
    "./postinstall.js"
  ],
  "scripts": {
    "build": "oclif-index && node --run fmt && ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "check": "ts-builder check --target node",
    "formatter:check": "ts-builder formatter --check",
    "linter:check": "ts-builder linter --check",
    "types:check": "ts-builder type-check --target node",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz",
    "fmt": "ts-builder format"
  },
  "dependencies": {
    "@milaboratories/pl-deployments": "workspace:*",
    "@oclif/core": "catalog:",
    "decompress": "catalog:",
    "readline-sync": "catalog:",
    "tar": "catalog:",
    "winston": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/oclif-index": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "@milaboratories/ts-configs": "workspace:*",
    "@types/decompress": "catalog:",
    "@types/node": "catalog:",
    "@types/readline-sync": "catalog:",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  },
  "oclif": {
    "bin": "pl-dev",
    "commands": {
      "identifier": "COMMANDS",
      "strategy": "explicit",
      "target": "./dist/index.js"
    },
    "dirname": "pl-service",
    "topicSeparator": " "
  }
}
