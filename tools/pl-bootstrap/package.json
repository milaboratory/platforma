{
  "name": "@platforma-sdk/bootstrap",
  "version": "2.0.1",
  "pl-version": "1.13.0",
  "description": "MiLaboratories Platforma Backend bootstrap code: configuration generators, service run wrappers and so on",
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "vite build",
    "test": "jest --passWithNoTests"
  },
  "bin": {
    "pl-service": "./bin/run.js"
  },
  "files": [
    "bin/run.cmd",
    "bin/run.js",
    "dist",
    "assets",
    "postinstall.js"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/decompress": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/readline-sync": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
    "ts-jest": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "dependencies": {
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "decompress": "catalog:",
    "readline-sync": "catalog:",
    "tar": "catalog:",
    "winston": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "oclif": {
    "bin": "pl-service",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-service",
    "topicSeparator": " "
  }
}
