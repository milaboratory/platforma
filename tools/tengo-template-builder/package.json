{
  "name": "@milaboratories/tengo-template-builder",
  "version": "1.14.5",
  "description": "Pl Tengo Template Builder",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "bin": {
    "pl-tengo": "./bin/run.js"
  },
  "files": [
    "./dist/**/*",
    "README.md"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratories/tengo-tester": "catalog:",
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "typescript": "catalog:",
    "tsup": "catalog:"
  },
  "oclif": {
    "bin": "pl-tengo",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-tengo",
    "topicSeparator": " "
  }
}
