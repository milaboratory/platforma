{
  "name": "@milaboratories/tengo-template-builder",
  "version": "1.14.6",
  "description": "Pl Tengo Template Builder",
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "vite build",
    "test": "jest"
  },
  "bin": {
    "pl-tengo": "./bin/run.js"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratories/tengo-tester": "catalog:",
    "@oclif/core": "catalog:",
    "canonicalize": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "typescript": "catalog:",
    "vite": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:"
  },
  "oclif": {
    "bin": "pl-tengo",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "dirname": "pl-tengo",
    "topicSeparator": " "
  }
}
