{
    "name": "@the-software",
    "version": "1.2.3",
    "block-software": {
      "entrypoints": {
        "pAsset": {
          "asset": {
            "type": "asset",
              "registry": {
                  "downloadURL": "http://example.com/registry"
              },
            "root": "./src"
          }
        },
        "custom-name": {
          "binary": {
            "artifact": {
              "registry": {
                  "downloadURL": "http://example.com/registry"
              },
              "name": "custom-package-name-1",
              "version": "4.4.4",
              "type": "binary",
              "root": "./src"
            },
            "cmd": ["aaaa"]
          }
        },
        "java-test-entrypoint": {
          "environment": { 
            "artifact": {
              "registry": {
                 "downloadURL": "http://example.com/registry"
              },
              "type": "environment",
              "runtime": "java",
              "root": "./src",
              "binDir": "."
            }
          }
        },
        "java-dep": {
          "binary": {
            "artifact": {
              "registry": {
                  "downloadURL": "http://example.com/registry"
              },
              "root": "./src",
              "type": "java",
              "environment": ":java-test-entrypoint"
            },
            "cmd": ["aaaa"]
          },
          "docker": {
            "artifact": {
              "type": "docker",
              "dockerfile": "Dockerfile",
              "context": ".",
              "entrypoint": ["/usr/bin/env", "printf"]
            },
            "cmd": ["echo", "hello"]
          }
        },
        "docker-test-entrypoint": {
          "docker": {
            "artifact": {
              "type": "docker",
              "dockerfile": "Dockerfile",
              "context": ".",
              "entrypoint": ["/usr/bin/env", "printf"]
            },
            "cmd": ["echo", "hello"]
          }
        }
      }
    }
}
