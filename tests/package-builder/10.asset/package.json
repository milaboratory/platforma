{
  "name": "@milaboratories/example-pl-package-asset-1",
  "version": "1.1.0",
  "private": true,
  "description": "Example of software to be used as runenv",
  "scripts": {
    "cleanup": "rm -rf ./dist/ && rm -f ./pkg-*.tgz",
    "reset": "npm run cleanup && rm -rf ./node_modules/",
    "build": "rm -rf dist && env -u PL_PKG_DEV pl-pkg build --entrypoint=1.2.3"
  },
  "block-software": {
    "registries": {
      "binary": {
        "example": {
          "downloadURL": "https://example.com/base-path/"
        }
      }
    },
    "entrypoints": {
      "1.2.3": {
        "asset": {
          "registry": "example",
          "name": "common/example",
          "version": "1.2.3",
          "root": "./src"
        }
      }
    }
  },
  "files": [
    "dist/"
  ],
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@platforma-sdk/package-builder": "file:../../../tools/package-builder"
  }
}
