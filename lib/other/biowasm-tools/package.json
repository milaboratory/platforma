{
  "name": "@milaboratories/biowasm-tools",
  "version": "2.0.2",
  "files": [
    "dist"
  ],
  "exports": {
    "./kalign": {
      "types": "./dist/kalign.d.ts",
      "import": "./dist/kalign.mjs"
    },
    "./rapidnj": {
      "types": "./dist/rapidnj.d.ts",
      "import": "./dist/rapidnj.mjs"
    }
  },
  "scripts": {
    "dev": "ts-builder serve --target browser-lib --build-config ./build.config.ts",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz",
    "build": "ts-builder build --target browser-lib --build-config ./build.config.ts",
    "check": "ts-builder check --target browser-lib",
    "formatter:check": "ts-builder formatter --check",
    "linter:check": "ts-builder linter --check",
    "types:check": "ts-builder type-check --target browser-lib",
    "generate-assets": "docker build --output=assets .",
    "test": "vitest run --coverage",
    "fmt": "ts-builder format"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "@milaboratories/ts-configs": "workspace:*",
    "@vitest/coverage-istanbul": "catalog:",
    "typescript": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  }
}
