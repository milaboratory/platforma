{
  "name": "@milaboratory/platforma-uikit",
  "version": "0.2.0",
  "type": "module",
  "engines": {
    "node": "^20.12.2",
    "npm": "^10.5.2"
  },
  "workspaces": [
    "lib",
    "playground"
  ],
  "scripts": {
    "dev": "npm run dev -w playground",
    "build:playground": "npm run build -w playground",
    "build": "npm run build -w lib",
    "build-watch": "npm run build-watch -w lib",
    "build-demo": "vite build -c vite.demo.config.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint . --ext js,ts,vue",
    "install-no-ws": "npm i --package-lock-only --workspaces false"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/dist/pl-uikit.umd.cjs",
  "module": "./lib/dist/pl-uikit.js",
  "types": "./lib/dist/lib.d.ts",
  "dependencies": {
    "@milaboratory/helpers": "^1.4.0",
    "@milaboratory/platforma-core": "^0.1.2",
    "js-yaml": "^4.1.0",
    "vue": "^3.4.27",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.4",
    "sass": "^1.69.5",
    "stylelint-config-prettier": "^9.0.5",
    "svgo": "^3.3.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vite-plugin-dts": "3.6.3",
    "vue-tsc": "^2.0.6"
  }
}
