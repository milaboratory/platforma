{
  "name": "@milaboratory/platforma-uikit",
  "version": "0.1.7",
  "type": "module",
  "engines": {
    "node": "^20.12.2",
    "npm": "^10.5.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-watch": "vue-tsc && vite build --watch",
    "build-demo": "vite build -c vite.demo.config.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint . --ext js,ts,vue",
    "install-no-ws": "npm i --package-lock-only --workspaces false",
    "parse-icons-folder": "node scripts/parse-icons.js",
    "minify-icons": "svgo -f src/lib/assets/icons/icon-assets -o src/lib/assets/icons/icon-assets-min",
    "generate-icons": "npm run parse-icons-folder && npm run minify-icons"
  },
  "files": [
    "dist",
    "src/assets"
  ],
  "main": "./dist/pl-uikit.umd.cjs",
  "module": "./dist/pl-uikit.js",
  "types": "./dist/lib.d.ts",
  "dependencies": {
    "@milaboratory/helpers": "^1.3.9",
    "@milaboratory/platforma-core": "^0.1.2",
    "js-yaml": "^4.1.0",
    "vue": "^3.4.27",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.4",
    "sass": "^1.69.5",
    "stylelint-config-prettier": "^9.0.5",
    "svgo": "^3.3.2",
    "typescript": "^5.4",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.6.3",
    "vue-tsc": "^2.0.6"
  }
}