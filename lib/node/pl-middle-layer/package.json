{
  "name": "@milaboratory/pl-project-state",
  "version": "1.2.0",
  "description": "Pl Project State",
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "scripts": {
    "build": "tsup",
    "readme": "exec oclif readme",
    "test": "jest"
  },
  "bin": {
    "pl-state": "./bin/run.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratory/computable": "^1.1.1",
    "@milaboratory/pl-client-v2": "^2.3.13",
    "@milaboratory/pl-tree": "^1.0.7",
    "@milaboratory/ts-helpers": "^1.0.11",
    "undici": "^6.18.2",
    "@oclif/core": "^3.26.9",
    "utility-types": "^3.11.0",
    "yaml": "^2.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.1",
    "dependency-cruiser": "^16.3.2",
    "ts-jest": "^29.1.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "tsup": {
    "entry": {
      "cli": "src/cmd/index.ts",
      "lib": "src/index.ts"
    },
    "splitting": false,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  },
  "oclif": {
    "commands": {
      "strategy": "explicit",
      "target": "./dist/cli.js",
      "identifier": "COMMANDS"
    },
    "bin": "pl-state",
    "dirname": "pl-state",
    "topicSeparator": " "
  }
}
