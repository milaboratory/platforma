{
  "name": "@milaboratory/pl-middle-layer",
  "version": "1.3.2",
  "description": "Pl Middle Layer",
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "scripts": {
    "build": "tsup",
    "readme": "exec oclif readme",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "bin": {
    "pl-state": "./bin/run.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "@milaboratory/computable": "^2.0.10",
    "@milaboratory/pframes-node": "^0.3.4",
    "@milaboratory/pl-block-registry": "^1.3.6",
    "@milaboratory/pl-client-v2": "^2.4.4",
    "@milaboratory/pl-drivers": "^1.0.36",
    "@milaboratory/pl-middle-layer-model": "^1.0.11",
    "@milaboratory/pl-tree": "^1.2.8",
    "@milaboratory/sdk-ui": "^0.8.0",
    "@milaboratory/ts-helpers": "^1.0.16",
    "@oclif/core": "^3.26.9",
    "canonicalize": "^2.0.0",
    "denque": "^2.1.0",
    "quickjs-emscripten": "^0.29.2",
    "undici": "^6.19.0",
    "utility-types": "^3.11.0",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.7",
    "dependency-cruiser": "^16.3.2",
    "ts-jest": "^29.1.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "tsup": {
    "entry": {
      "cli": "src/cmd/index.ts",
      "lib": "src/index.ts"
    },
    "splitting": false,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  },
  "oclif": {
    "commands": {
      "strategy": "explicit",
      "target": "./dist/cli.js",
      "identifier": "COMMANDS"
    },
    "bin": "pl-state",
    "dirname": "pl-state",
    "topicSeparator": " "
  }
}
