{
  "name": "@milaboratories/pl-config",
  "version": "1.7.13",
  "description": "MiLaboratories Platforma Backend and Middle Layer config generator",
  "keywords": [],
  "license": "UNLICENSED",
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md",
    "package.json",
    "bin/run.cmd",
    "bin/run.js",
    "assets",
    "postinstall.js"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "check": "ts-builder check --target node",
    "formatter:check": "ts-builder formatter --check",
    "linter:check": "ts-builder linter --check",
    "types:check": "ts-builder type-check --target node",
    "test": "vitest run --coverage",
    "do-pack": "rm -rf src/.test && rm -f *.tgz && pnpm pack && mv *.tgz package.tgz",
    "fmt": "ts-builder format"
  },
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:*",
    "upath": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "@milaboratories/ts-configs": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
