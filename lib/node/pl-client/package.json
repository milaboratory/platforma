{
  "name": "@milaboratory/pl-client-v2",
  "version": "2.4.10",
  "description": "New TS/JS client for Platform API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build-debug": "tsc",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand",
    "test-setup": "tsup --no-config src/cmd/test_setup.ts --out-dir test_scripts && node test_scripts/test_setup.js",
    "pretty": "prettier --write \"./src/**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@milaboratory/ts-helpers": "^1.0.19",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "canonicalize": "^2.0.0",
    "denque": "^2.1.0",
    "https-proxy-agent": "^7.0.5",
    "long": "^5.2.3",
    "undici": "^6.19.8",
    "utility-types": "^3.11.0",
    "yaml": "^2.5.0"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@types/http-proxy": "^1.17.15",
    "@types/jest": "^29.5.12",
    "jest": "^29.6.4",
    "ts-jest": "^29.2.5",
    "tsup": "~8.1.2",
    "typescript": "5.5.x",
    "prettier": "^3.3.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
