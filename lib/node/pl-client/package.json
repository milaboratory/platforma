{
  "name": "@milaboratory/pl-client-v2",
  "version": "2.4.10",
  "description": "New TS/JS client for Platform API",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build-debug": "tsc",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "@milaboratory/ts-helpers": "workspace:^",
    "@grpc/grpc-js": "catalog:",
    "@protobuf-ts/grpc-transport": "catalog:",
    "@protobuf-ts/runtime": "catalog:",
    "@protobuf-ts/runtime-rpc": "catalog:",
    "canonicalize": "catalog:",
    "denque": "catalog:",
    "https-proxy-agent": "catalog:",
    "long": "catalog:",
    "undici": "catalog:",
    "utility-types": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "typescript": "catalog:",
    "tsup": "catalog:",
    "@protobuf-ts/plugin": "catalog:",
    "@types/http-proxy": "catalog:",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "prettier": "catalog:"
  }
}
