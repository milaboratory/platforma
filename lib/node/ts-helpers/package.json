{
  "name": "@milaboratories/ts-helpers",
  "version": "1.6.0",
  "description": "Useful helpers for our TS projects",
  "engines": {
    "node": ">=22.19.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "ts-builder types --target node",
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "canonicalize": "catalog:",
    "denque": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "typescript": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}
