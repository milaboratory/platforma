{
  "name": "@milaboratory/ts-helpers",
  "version": "0.0.4",
  "description": "Useful helpers for our TS projects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand --passWithNoTests"
  },
  "dependencies": {},
  "peerDependencies": {
    "winston": "3.x",
    "@oclif/core": "3.x"
  },
  "peerDependenciesMeta": {
    "winston": {
      "optional": true
    },
    "@oclif/core": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",

    "winston": "3.x",
    "@oclif/core": "3.x"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
