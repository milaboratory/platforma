{
  "name": "@milaboratories/ts-helpers",
  "version": "1.0.28",
  "description": "Useful helpers for our TS projects",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "vite build",
    "test": "jest --runInBand",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "denque": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "typescript": "catalog:",
    "vite": "catalog:",
    "@types/node": "catalog:",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "@jest/globals": "catalog:",
    "ts-jest": "catalog:"
  }
}
