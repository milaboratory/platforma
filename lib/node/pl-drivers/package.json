{
  "name": "@milaboratory/pl-drivers",
  "version": "1.2.4",
  "description": "Drivers and a low-level clients for log streaming, downloading and uploading files from and to Platforma",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.11",
    "@milaboratory/computable": "^2.1.0",
    "@milaboratory/pl-client-v2": "^2.4.7",
    "@milaboratory/pl-tree": "^1.3.3",
    "@milaboratory/sdk-model": "^1.3.7",
    "@milaboratory/ts-helpers": "^1.0.19",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "denque": "^2.1.0",
    "tar-fs": "^3.0.6",
    "undici": "^6.19.2",
    "zod": "^3.23.8"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/tar-fs": "^2.0.4",
    "jest": "^29.6.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
