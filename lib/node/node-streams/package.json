{
  "name": "@milaboratories/node-streams",
  "version": "1.0.7",
  "description": "",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "type-check": "ts-builder types --target node",
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "lint": "ts-builder lint --target node",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz",
    "fmt": "ts-builder format"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}
