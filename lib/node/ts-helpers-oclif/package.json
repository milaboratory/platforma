{
  "name": "@milaboratories/ts-helpers-oclif",
  "version": "1.1.30",
  "description": "Useful helpers for our TS projects (oclif-specific)",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "ts-builder build --target node",
    "type-check": "ts-builder types --target node",
    "lint": "eslint .",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:*",
    "@oclif/core": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/ts-builder": "workspace:*",
    "@milaboratories/build-configs": "workspace:*",
    "typescript": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}