  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "@milaboratories/computable": "workspace:^",
    "@milaboratories/pl-client": "workspace:^",
    "@milaboratories/ts-helpers": "workspace:^",
    "denque": "catalog:",
    "utility-types": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
