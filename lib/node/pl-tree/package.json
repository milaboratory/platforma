{
  "name": "@milaboratories/pl-tree",
  "version": "1.3.6",
  "description": "Reactive pl tree state",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
