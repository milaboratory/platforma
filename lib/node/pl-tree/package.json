{
  "name": "@milaboratory/pl-tree",
  "version": "1.3.3",
  "description": "Reactive pl tree state",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand",
    "pretty": "prettier --write \"./src/**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "dependencies": {
    "@milaboratory/computable": "^2.1.0",
    "@milaboratory/pl-client-v2": "^2.4.6",
    "@milaboratory/ts-helpers": "^1.0.18",
    "denque": "^2.1.0",
    "utility-types": "^3.11.0",
    "zod": "^3.23.8"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.6.4",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.2",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
