{
  "name": "@milaboratory/pl-tree",
  "version": "1.0.4",
  "description": "Reactive pl tree state",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@milaboratory/computable": "1.1.1",
    "@milaboratory/pl-client-v2": "2.3.12",
    "@milaboratory/ts-helpers": "1.0.11",
    "denque": "^2.1.0",
    "utility-types": "^3.11.0"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
