{
  "name": "@milaboratories/pl-local",
  "version": "1.8.2",
  "pl-version": "1.16.2",
  "description": "MiLaboratories Platforma Backend code service run wrapper",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "vite build",
    "test": "vitest",
    "do-pack": "rm -rf src/.test && rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*",
    "README.md",
    "bin/run.cmd",
    "bin/run.js",
    "assets",
    "postinstall.js"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "vite": "catalog:",
    "vitest": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@types/decompress": "catalog:",
    "prettier": "catalog:",
    "ts-jest": "catalog:",
    "jest": "catalog:",
    "tsconfig-paths": "catalog:",
    "utility-types": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "decompress": "catalog:",
    "undici": "catalog:",
    "@milaboratories/ts-helpers": "workspace:^",
    "tar": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  }
}
