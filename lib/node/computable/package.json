{
  "name": "@milaboratories/computable",
  "version": "2.1.2",
  "description": "Watcher / Computable Abstraction",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:^",
    "@types/node": "catalog:",
    "utility-types": "catalog:"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
