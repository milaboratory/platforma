{
  "name": "@milaboratory/computable",
  "version": "1.0.7",
  "description": "Watcher / Computable Abstraction",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch",
    "type-check": "tsc --noEmit --composite false",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@milaboratory/ts-helpers": "^1.0.10",
    "utility-types": "^3.11.0"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
