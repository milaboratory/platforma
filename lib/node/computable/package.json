{
  "name": "@milaboratories/computable",
  "version": "2.4.7",
  "engines": {
    "node": ">=20.0.0"
  },
  "description": "Watcher / Computable Abstraction",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit --composite false",
    "build": "vite build",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:^",
    "@milaboratories/pl-error-like": "workspace:^",
    "@types/node": "catalog:",
    "utility-types": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "typescript": "catalog:",
    "vite": "catalog:",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "@jest/globals": "catalog:",
    "ts-jest": "catalog:"
  }
}
