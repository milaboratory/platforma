{
  "name": "@milaboratories/computable",
  "version": "2.6.8",
  "engines": {
    "node": ">=22.19.0"
  },
  "description": "Watcher / Computable Abstraction",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "ts-builder types --target node",
    "build": "ts-builder build --target node",
    "watch": "ts-builder build --target node --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "do-pack": "rm -f *.tgz && pnpm pack && mv *.tgz package.tgz"
  },
  "files": [
    "./dist/**/*",
    "./src/**/*"
  ],
  "dependencies": {
    "@milaboratories/ts-helpers": "workspace:*",
    "@milaboratories/pl-error-like": "workspace:*",
    "@types/node": "catalog:",
    "utility-types": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/build-configs": "workspace:*",
    "@milaboratories/ts-builder": "workspace:*",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@milaboratories/ts-configs": "workspace:*"
  }
}