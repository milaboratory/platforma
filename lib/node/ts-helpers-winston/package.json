{
  "name": "@milaboratory/ts-helpers-winston",
  "version": "1.0.0",
  "description": "Useful helpers for our TS projects (winston-specific)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build-watch": "tsup --watch"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@milaboratory/ts-helpers": "workspace:^",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@milaboratories/platforma-build-configs": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
