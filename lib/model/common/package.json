{
  "name": "@milaboratory/sdk-model",
  "version": "1.3.6",
  "description": "Platforma SDK Model",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "devDependencies": {
    "prettier": "^3.3.3",
    "tsup": "8.1.2",
    "typescript": "^5.5.3"
  },
  "tsup": {
    "entry": {
      "index": "src/index.ts"
    },
    "splitting": false,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}