{
    "private": true,
    "name": "@mi-tests/tengo-integration-2",
    "version": "1.0.0",
    "description": "Simple static artifacts for tests (assets and software)",
    "scripts": {
        "cleanup": "rm -rf ./dist/ && rm -f ./pkg-*.tgz",
        "test:check": "pnpm run cleanup && pl-pkg build"
    },
    "block-software": {
        "entrypoints": {
            "software": {
                "binary": {
                    "artifact": {
                        "registry": "publish-never-happen",
                        "type": "binary",
                        "roots": {
                            "linux-x64": "software-root",
                            "linux-aarch64": "software-root",
                            "macosx-x64": "software-root",
                            "macosx-aarch64": "software-root",
                            "windows-x64": "software-root"
                        }
                    },
                    "cmd": [
                        "fake-binary"
                    ]
                }
            },
            "asset": {
                "asset": {
                    "registry": {
                        "name": "publish-never-happen",
                        "downloadURL": "http://example.com/unknown-asset"
                    },
                    "root": "asset-root"
                }
            }
        }
    },
    "license": "UNLICENSED",
    "devDependencies": {
        "@platforma-sdk/package-builder": "workspace:*"
    }
}
