{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": ["./dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"],
      "passThroughEnv": [
        "PL_ADDRESS",
        "PL_TEST_PASSWORD",
        "PL_TEST_USER",
        "PL_TEST_PROXY"
      ]
    }
  }
}
